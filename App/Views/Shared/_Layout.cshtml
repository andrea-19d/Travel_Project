@using System.Web.Optimization;

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <title>Travela - Tourism Website Template</title>
    <meta content="width=device-width, initial-scale=1.0" name="viewport">
    <meta content="" name="keywords">
    <meta content="" name="description">

    <!-- Google Web Fonts -->
    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=Jost:wght@500;600&family=Roboto&display=swap" rel="stylesheet">

    <!-- Icon Font Stylesheet -->
    <link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.15.4/css/all.css" />
    <link href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.4.1/font/bootstrap-icons.css" rel="stylesheet">


    <!-- Libraries Stylesheet -->
    @Styles.Render("~/bundle/owlcarousel/css")
    @Styles.Render("~/bundle/lightbox/css")

    <!-- Customized Bootstrap Stylesheet -->
    @Styles.Render("~/bundle/bootstrap/min/css")

    <!-- Template Stylesheet -->
    @Styles.Render("~/bundle/style/css")

    @if(IsSectionDefined("Styles"))
    { @RenderSection("Styles", required: false)}

</head>


<body>
    <!-- Spinner Start  idk when to put it  -->

    <!-- Spinner End -->
    <!-- Topbar Start -->
    @Html.Partial("_Topbar")
    <!-- Topbar End -->
    <!-- Navbar & Hero Start -->
    @Html.Partial("_NavbarHero")
    <!-- Navbar & Hero End -->
    <!-- Header Start -->
    @Html.Partial("_Header")/
    <!-- Header End -->
    <!-- Content -->
    @RenderBody();
    <!-- End Content -->
    <!-- Subscribe Start -->
    @Html.Partial("_Subscribe")
    <!-- Subscribe End -->
    <!-- Footer Start -->

    @Html.Partial("_Footer")

    <!-- Footer End -->
    <!-- Copyright Start -->

    @Html.Partial("_Copyright")
    <!-- Copyright End -->

    <!-- Back to Top -->
    <a href="#" class="btn btn-primary btn-primary-outline-0 btn-md-square back-to-top"><i class="fa fa-arrow-up"></i></a>


    <!-- JavaScript Libraries -->
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.6.4/jquery.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.0/dist/js/bootstrap.bundle.min.js"></script>
    @Scripts.Render("~/bundle/easing")
    @Scripts.Render("~/bundle/waypoints")
    @Scripts.Render("~/bundle/owlcarousel")
    @Scripts.Render("~/bundle/lightbox")


    <!-- Template Javascript -->
    @Scripts.Render("~/bundle/main")

    @RenderSection("scripts", required: false);


</body>
</html>